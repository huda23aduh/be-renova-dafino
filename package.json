{
  "name": "express-used-car",
  "version": "1.0.0",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/app.js",
    "start": "node src/app.js",
    "seed": "npx sequelize-cli db:seed:all --config config/config.cjs",
    "seed:undo": "npx sequelize-cli db:seed:undo:all --config config/config.cjs",
    "migrate": "npx sequelize-cli db:migrate --config config/config.cjs",
    "migrate:undo": "npx sequelize-cli db:migrate:undo:all --config config/config.cjs",
    "migrate:reset": "npm run migrate:undo && npm run migrate",
    "db:reset": "npm run migrate:undo && npm run migrate && npm run seed"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "mysql2": "^3.9.3",
    "sequelize": "^6.37.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
